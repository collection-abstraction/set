{
  "name": "@collection-abstraction/set",
  "description": "Python's set data structure for JavaScript",
  "version": "4.0.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://collection-abstraction.github.io/set",
  "repository": {
    "url": "https://github.com/collection-abstraction/set",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/collection-abstraction/set/issues"
  },
  "keywords": [
    "collection",
    "collections",
    "javascript",
    "js",
    "set"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "lint": "true",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "dependencies": {
    "@aureooms/js-error": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.12.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "np": "7.4.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "0.52.3"
  },
  "ava": {
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  }
}
